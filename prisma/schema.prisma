generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["mongoDb"]
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model TrackTimes {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  game        String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
  time        String
  track       String
  vehicle     String
}
